CFLAGS += -I$(PARAM_DIR)
CFLAGS += -I$(PARAM_DIR)/pointer/
CFLAGS += -I$(PARAM_DIR)/pointer/smart_pointer/
CFLAGS += -I$(PARAM_DIR)/pointer/shared_pointer/
CFLAGS += -I$(PARAM_DIR)/exception/
CFLAGS += -I$(PARAM_DIR)/array/
CFLAGS += -I$(PARAM_DIR)/array/dynamic_array/
CFLAGS += -I$(PARAM_DIR)/array/static_array/
CFLAGS += -I$(PARAM_DIR)/list/
CFLAGS += -I$(PARAM_DIR)/list/seq_list/
CFLAGS += -I$(PARAM_DIR)/list/seq_list/dynamic_list/
CFLAGS += -I$(PARAM_DIR)/list/seq_list/static_list/
CFLAGS += -I$(PARAM_DIR)/list/link_list/
CFLAGS += -I$(PARAM_DIR)/list/link_list/static_link_list/
CFLAGS += -I$(PARAM_DIR)/list/link_list/circle_list/
CFLAGS += -I$(PARAM_DIR)/list/dual_link_list/
CFLAGS += -I$(PARAM_DIR)/list/dual_link_list/dual_circle_list/
CFLAGS += -I$(PARAM_DIR)/list/kernel_list
CFLAGS += -I$(PARAM_DIR)/stack_and_queue/stack/
CFLAGS += -I$(PARAM_DIR)/stack_and_queue/stack/static_stack/
