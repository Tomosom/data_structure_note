CFLAGS += -I$(PARAM_DIR)/tree/btree/
CFLAGS += -I$(PARAM_DIR)/graph/
CFLAGS += -I$(PARAM_DIR)/graph/matrix_graph/
CFLAGS += -I$(PARAM_DIR)/graph/list_graph/

SRC := $(PARAM_DIR)/object.cpp
SRC += $(PARAM_DIR)/exception/exception.cpp
